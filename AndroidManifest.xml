<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reimbursementapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="10" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.reimbursementapp.HomeScreenActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.reimbursementapp.ReceiptPhotoActivity"
            android:label="@string/title_activity_receipt_photo" >
        </activity>
        <activity
            android:name="com.example.reimbursementapp.ManageReceiptsActivity"
            android:label="@string/title_activity_manage_receipts" >
        </activity>
        <activity
            android:name="com.example.reimbursementapp.SingleListItem"
            android:label="@string/title_activity_single_list_item" >
        </activity>
        <activity
            android:name="com.example.reimbursementapp.PaymentHistoryActivity"
            android:label="@string/title_activity_payment_history" >
        </activity>
        <activity
            android:name="com.example.reimbursementapp.SingleHistoryTime"
            android:label="@string/title_activity_single_history_time" >
        </activity>
        <activity
            android:name="com.example.reimbursementapp.SingleHistoryItem"
            android:label="@string/title_activity_single_history_item" >
        </activity>
    </application>

</manifest>